:test,

references {
	
	e1 {
		property 1
	},
	
	e2 {
		property "a"
	},
	
	e3(p) {
		property = p
	}
	
	values ['10', '11']
},

entities {
	
	*for(n in [1, 2]) 
	_name = "entity" + n {
	
		property1 "a"
		
	},
	
	entity3 {
	
		property1 *for(i in ['a', 'b']) : i
		
	},
	
	
	entity4 {
		cplx *for(i in ['1', '2']) {
			property = i
		}
	},
	
	
	entity5 {
	
		cplx *for(i in ['1', '2']) _name = "property" + i {
			property = i
		}
		
	},
	
	entity6 {
		*for(i in ['1', '2']) _name = "property" + i : i
	},
	
	entity7 {
		*for(i in ['1', '2']) _name = "cplx" + i @e2
	},
	
	entity8 {
		*for(i in ['1', '2']) _name = "cplx" + i @e3(i)
	},
	
	entity9 {
		*for(i in ['1', '2'])
		_name = "cplx" + i @e3(i) {
			newProperty = i + 'a'
		}
	}
	
	*for(string i in rootOv.getArray("references.values")) _name = "entity" + i {
		property "a"
	}
	
	*for(i in ['12', '13']) _name = "entity" + i {
		*for(j in ['1', '2']) _name = "cplx" + j {
			property "a"
		}
	}
	
	*for(i in ['14', '15']) _name = "entity" + i {
		*for(j in ['1', '2']) _name = "cplx" + i + j {
			property "a"
		}
	}
	
}